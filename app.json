{
  "expo": {
    "name": "HomeCare",
    "slug": "homecare-demo",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "transparencyhome",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.transparencyhome",
      "infoPlist": {
        "NSMicrophoneUsageDescription": "We use the microphone for speech recognition",
        "NSSpeechRecognitionUsageDescription": "We transcribe your speech to text",
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": true
        }
      }
    },
    "android": {
      "permissions": [
        "RECORD_AUDIO",
        "RECORD_AUDIO"
      ],
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.anonymous.transparencyhome",
      "usesCleartextTraffic": true
    },
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "owner": "viinivahti",
    "extra": {
      "eas": {
        "projectId": "75dc4087-cb1d-42e3-8b07-a17c67579ee1"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/75dc4087-cb1d-42e3-8b07-a17c67579ee1"
    }
  }
}
